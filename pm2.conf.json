
{
  "apps": {
      "name": "Journal",
      "exec": "app.js",
      "watch": true,
      "instances": 3,
      "autorestart": true,
      "ignore_watch": [
        "node_modules",
        "logs"
      ],
      "error_file": "logs/err.log",
      "out_file": "logs/out.log",
      "log_date_format": "YYYY-MM-DD HH:mm:ss"
    }
}
